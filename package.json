{
	"name": "n8n-nodes-qualitor",
	"version": "0.1.0",
	"description": "n8n Qualitor SOAP Api Node",
	"keywords": [
		"n8n-community-node-package",
		"qualitor"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Junior Fontenele",
		"email": "n8n@juniorfontenele.com.br"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/juniorfontenele/n8n-qualitor.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/QualitorApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Qualitor/Qualitor.node.js"
		]
	},
	"devDependencies": {
		"@types/qs": "^6.14.0",
		"@types/xml2js": "^0.4.14",
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"html-entities": "^2.6.0",
		"qs": "^6.14.0",
		"xml2js": "^0.6.2"
	}
}
